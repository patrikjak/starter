{
    "name": "patrikjak/starter",
    "description": "Laravel app starter - admin panel",
    "type": "library",
	"version": "0.3.0",
    "require": {
        "php": "^8.4",
        "laravel/framework": "^12.1.1",
        "patrikjak/utils": "^2.5.0",
        "patrikjak/auth": "^1.4.0"
    },
    "require-dev": {
        "orchestra/testbench": "^10.0.0",
        "phpstan/phpstan": "^2.1.0",
        "phpunit/phpunit": "^11.0",
        "slevomat/coding-standard": "~8.0",
        "spatie/phpunit-snapshot-assertions": "^5.1",
        "larastan/larastan": "^3.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Patrikjak\\Starter\\": "src/",
            "Patrikjak\\Starter\\Database\\Factories\\": "database/factories/",
            "Patrikjak\\Starter\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Patrik Jakab"
        }
    ],
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
